{
    "workbench.iconTheme": "vscode-great-icons",
    "git.openRepositoryInParentFolders": "always",
    "workbench.colorCustomizations": {
        "activityBar.background": "#3B1D61",
        "titleBar.activeBackground": "#532888",
        "titleBar.activeForeground": "#FBF9FD"
    },
    "editor.codeLens": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "files.autoSave": "afterDelay",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "csharp.suppressBuildAssetsNotification": true,
    "git.postCommitCommand": "sync",
    "sessionlogger.enabled": false,
    "csharp.debug.console": "externalTerminal",
    "csharp.referencesCodeLens.enabled": false,
    "explorer.compactFolders": false,
    "explorer.sortOrderLexicographicOptions": "unicode",
    "markdox.urls": [
        "https://github.com/krank/csharp-ref.git",
        "https://gitlab.com/karye/prrprr01-te.git"
    ],
    "workbench.editorAssociations": {
        "*.md": "vscode.markdown.preview.editor"
    }
}
